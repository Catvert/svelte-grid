{
  "name": "svelte-grid",
  "version": "0.1.0",
  "description": "A responsive, draggable and resizable grid layout, for Svelte.",
  "svelte": "src/index.svelte",
  "module": "index.mjs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "pretty": "prettier --config .prettierrc --write \"rollup.config.js\" \".eslintrc\" \"package.json\" \"src/**/*.js\" ",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "go": "npm-run-all pretty lint npm run build",
    "preversion": "npm run go"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaheqelyan/svelte-grid.git"
  },
  "keywords": ["svelte", "grid", "drag", "draggable", "resize", "resizable", "fluid", "responsive"],
  "author": "Vahe Araqelyan <vahe.araqelyan.2013@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaheqelyan/svelte-grid/issues"
  },
  "homepage": "https://github.com/vaheqelyan/svelte-grid#readme",
  "devDependencies": {
    "eslint": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.18.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.6.10"
  },
  "files": ["src", "index.mjs", "index.js"]
}
